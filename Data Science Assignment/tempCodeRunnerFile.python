def print_pattern(n, N):
    if n <= 0:
        print(n, end=' ')
        return
    print(n, end=' ')
    print_pattern(n - 5, N)
    # if n != N:
    print(n, end=' ')

# Taking input
N = int(input())

# Printing pattern
print_pattern(N, N)
